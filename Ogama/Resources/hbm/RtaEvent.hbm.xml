<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"

                           assembly="Ogama"

                           namespace="Ogama.Modules.Rta.Model">

  <class name="RtaEvent" table="RtaEvent" lazy="false">

    <id name="ID">
      <generator class="guid"/>
    </id>

    

    <many-to-one
        name="fkRtaCategory"                                
        column="fkRtaCategory"                              
        class="RtaCategory"                               
        cascade="none"
        fetch="select"                                
        update="true"
        insert="true"
        access="property"
        unique="true"                         
        optimistic-lock="false"                
        not-found="exception"                
  />


    <property name="Xstart">
      <column name="Xstart" />
    </property>

    <property name="Xend">
      <column name="Xend" />
    </property>

    <property name="startTimestamp">
      <column name="startTimestamp" />
    </property>

    <property name="endTimestamp">
      <column name="endTimestamp" />
    </property>
    
  </class>

</hibernate-mapping>