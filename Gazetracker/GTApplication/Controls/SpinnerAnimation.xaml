<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="GazeTrackerUI.Controls.SpinnerAnimation"
	x:Name="UserControl"
	Width="Auto" Height="Auto">

    <Grid>
        <Canvas x:Name="canvas2" Height="100" Width="100" Visibility="Collapsed">
          <Canvas Height="50" Width="50" x:Name="canvas1" RenderTransformOrigin=".5,.5">
             <Canvas.RenderTransform>
              <RotateTransform x:Name="spin" Angle="0"/>
             </Canvas.RenderTransform>
          </Canvas>
        </Canvas>
    </Grid>
	
</UserControl>